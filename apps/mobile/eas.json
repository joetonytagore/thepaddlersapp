{
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "STRIPE_PUBLISHABLE_KEY": "@stripe_publishable_key"
      },
      "ios": {
        "simulator": true
      },
      "android": {
        "buildType": "apk"
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "STRIPE_PUBLISHABLE_KEY": "@stripe_publishable_key"
      }
    },
    "production": {
      "env": {
        "STRIPE_PUBLISHABLE_KEY": "@stripe_publishable_key"
      },
      "ios": {
        "simulator": false
      },
      "android": {
        "buildType": "app-bundle"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
