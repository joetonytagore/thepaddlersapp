{
  "dashboard": {
    "id": null,
    "title": "ThePaddlers - Service Overview",
    "uid": "thepaddlers-overview",
    "timezone": "browser",
    "panels": [
      {
        "type": "graph",
        "title": "HTTP Requests (count)",
        "targets": [ { "expr": "sum(rate(http_server_requests_seconds_count{application=\"thepaddlers\"}[5m])) by (uri)", "legendFormat": "{{uri}}" } ],
        "gridPos": { "x": 0, "y": 0, "w": 12, "h": 8 }
      },
      {
        "type": "graph",
        "title": "HTTP p95 Latency (s)",
        "targets": [ { "expr": "histogram_quantile(0.95, sum(rate(http_server_requests_seconds_bucket{application=\"thepaddlers\"}[5m])) by (le))", "legendFormat": "p95" } ],
        "gridPos": { "x": 12, "y": 0, "w": 12, "h": 8 }
      },
      {
        "type": "graph",
        "title": "JVM Memory Used (MB)",
        "targets": [ { "expr": "jvm_memory_used_bytes{area=\"heap\"} / 1024 / 1024", "legendFormat": "heap" } ],
        "gridPos": { "x": 0, "y": 8, "w": 12, "h": 8 }
      },
      {
        "type": "graph",
        "title": "Booking Created Rate (per min)",
        "targets": [ { "expr": "sum(rate(thepaddlers_booking_created_total[5m]))", "legendFormat": "bookings/min" } ],
        "gridPos": { "x": 12, "y": 8, "w": 12, "h": 8 }
      }
    ],
    "schemaVersion": 27,
    "version": 1
  }
}

